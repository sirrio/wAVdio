{
  "name": "wavdio",
  "version": "0.2.0",
  "scripts": {
    "ng": "ng",
    "start": "node ./bin/server.js",
    "start:nolang": "ng build && node ./bin/server.js",
    "start:en": "ng build --aot --i18n-locale de --i18n-format xlf --i18n-file src/locale/messages.de.xlf --output-path dist/wAVdio && ng build --aot --i18n-locale en --i18n-format xlf --i18n-file src/locale/messages.en.xlf --output-path dist/wAVdio/en --base-href /en/ && node ./bin/server.js",
    "build:prod": "ng build --prod --aot --i18n-locale de --i18n-format xlf --i18n-file src/locale/messages.de.xlf --output-path dist/wAVdio && ng build --prod --aot --i18n-locale en --i18n-format xlf --i18n-file src/locale/messages.en.xlf --output-path dist/wAVdio/en --base-href /en/",
    "build": "ng build --aot --i18n-locale de --i18n-format xlf --i18n-file src/locale/messages.de.xlf --output-path dist/wAVdio && ng build --aot --i18n-locale en --i18n-format xlf --i18n-file src/locale/messages.en.xlf --output-path dist/wAVdio/en --base-href /en/",
    "test": "mocha",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "mocha": "mocha --reporter mocha-junit-reporter --reporter-options mochaFile=./test-results/mocha.xml --exit"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.2.8",
    "@angular/cdk": "^7.3.3",
    "@angular/common": "^7.2.7",
    "@angular/compiler": "^7.2.7",
    "@angular/core": "^7.2.8",
    "@angular/forms": "^7.2.7",
    "@angular/http": "^7.2.7",
    "@angular/platform-browser": "^7.2.7",
    "@angular/platform-browser-dynamic": "^7.2.7",
    "@angular/router": "^7.2.7",
    "@fortawesome/angular-fontawesome": "^0.3.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.15",
    "@fortawesome/free-solid-svg-icons": "^5.7.2",
    "@ng-bootstrap/ng-bootstrap": "^4.1.0",
    "@swimlane/ngx-charts": "^10.1.0",
    "@types/jquery": "^3.3.29",
    "@types/winston": "^2.4.4",
    "bluebird": "^3.5.3",
    "body-parser": "^1.18.3",
    "bootstrap": "^4.3.1",
    "core-js": "^2.6.5",
    "debug": "^4.1.1",
    "eslint": "^5.15.1",
    "express": "^4.16.4",
    "express-fileupload": "^1.0.0",
    "express-jwt": "^5.3.1",
    "fs": "0.0.1-security",
    "http-errors": "^1.7.1",
    "jquery": "^3.3.1",
    "jsonwebtoken": "^8.5.0",
    "mongoose": "^4.13.17",
    "morgan": "^1.9.1",
    "ngx-cookie-service": "^2.1.0",
    "ngx-print": "^1.1.4",
    "ngx-qrcode2": "0.0.9",
    "ngx-slick-carousel": "^0.4.4",
    "popper.js": "^1.14.7",
    "rotating-file-stream": "^1.4.0",
    "rxjs": "^6.4.0",
    "serve-favicon": "^2.5.0",
    "slick-carousel": "^1.8.1",
    "winston": "^3.2.1",
    "xss-clean": "^0.1.1",
    "zone.js": "^0.8.29"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.12.4",
